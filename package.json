{
  "devDependencies": {
    "tailwindcss": "^3.4.4"
  },
  "name": "yasapp",
  "version": "1.0.0",
  "description": "The Real Estate App - YASApp",
  "main": "tailwind.config.js",
  "directories": {
    "test": "tests"
  },
